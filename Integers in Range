class Solution(object):
    def isCovered(self, ranges, left, right):
        """
        :type ranges: List[List[int]]
        :type left: int
        :type right: int
        :rtype: bool
        """
        for i in range(left, right+1):

            for j in ranges:

                if (j[0] <= i <= j[1]):

                    break
            else:
                return False
        return True
